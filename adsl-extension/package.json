{
    "name": "adsl-extension",
    "displayName": "adsl-extension",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "adsl",
            "aliases": ["adsl", "adsl"],
            "extensions": [".adsl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "adsl",
            "scopeName": "source.adsl",
            "path": "./syntaxes/adsl.tmLanguage.json"
        }]
    }
}